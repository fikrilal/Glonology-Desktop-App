	<queryString>
		<![CDATA[SELECT
     detailpenjualan.`noFaktur`,
     detailpenjualan.`idBarang`,
     detailpenjualan.`jumlahBarang`,
     detailpenjualan.`hargaBarang`,
     detailpenjualan.`namaBarang`,
     faktur.`noFaktur`,
     faktur.`hargatotal`,
     faktur.`jumlahtotal`,
     faktur.`tanggal`,
     faktur.`userId`,
     faktur.`bayar`,
     faktur.`kembalian`
FROM
     `faktur` faktur INNER JOIN `detailpenjualan` detailpenjualan ON faktur.`noFaktur` = detailpenjualan.`noFaktur` 
where faktur.noFaktur = $P{a}]]>
	</queryString>
